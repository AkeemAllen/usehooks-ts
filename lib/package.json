{
  "name": "usehooks-ts",
  "private": false,
  "description": "React hook library, ready to use, written in Typescript.",
  "version": "2.4.1",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "homepage": "https://usehooks-ts.com",
  "keywords": [
    "typescript",
    "react",
    "react-hooks"
  ],
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "start": "chokidar \"src/**/*.ts\" -c \"npm run build\"",
    "build:tsup": "tsup ./src/index.ts --dts --sourcemap --format esm --clean",
    "build:tsc": "tsc",
    "build:rollup": "rollup -c",
    "build:types": "tsc -d --emitDeclarationOnly",
    "build": "npm run build:types && npm run build:rollup",
    "clean": "rimraf -rf ./dist",
    "types-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "react": ">=16.8.0",
    "react-test-renderer": ">=16.8.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliencrn/usehooks-ts"
  },
  "bugs": {
    "url": "https://github.com/juliencrn/usehooks-ts/issues"
  },
  "gitHead": "05340494dd6e96fa501695936b6fb51c051ffd40"
}
